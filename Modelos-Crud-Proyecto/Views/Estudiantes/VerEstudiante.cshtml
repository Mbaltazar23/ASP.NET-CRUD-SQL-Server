
@model Estudiante

<div class="modal" tabindex="-1" id="verEstudiante">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Estudiante</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="card-body">
                    <p><strong>@Html.DisplayNameFor(model => model.Codigo):</strong>&nbsp; &nbsp; @Html.DisplayFor(model => model.Codigo)</p>
                    <p><strong>@Html.DisplayNameFor(model => model.persona.Nombres):</strong>&nbsp; &nbsp;@Html.DisplayFor(model => model.persona.Nombres)</p>
                    <p><strong>@Html.DisplayNameFor(model => model.persona.Apellidos):</strong> &nbsp; &nbsp; @Html.DisplayFor(model => model.persona.Apellidos)</p>
                    <p><strong>@Html.DisplayNameFor(model => model.persona.Email):</strong> &nbsp; &nbsp; @Html.DisplayFor(model => model.persona.Email)</p>
                </div>
            </div>
        </div>
    </div>
</div>
